datasource db {
  provider = "sqlite"
  url      = "file:./tmdb_cache.db"
}

generator client {
  provider = "prisma-client-js"
}

model MovieCache {
  id         Int      @id @default(autoincrement())
  name       String
  hostname   String?
  stream_id  String?
  year       String
  tmdb_id    String?   @unique
  data       String
  updatedAt  DateTime @updatedAt
  createdAt  DateTime @default(now())
}